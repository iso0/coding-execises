def swap_case(s):
    my_list = list(s)    
    for i in range(len(my_list)):
        if s[i].isupper():
           my_list[i] = s[i].lower()
        else:
            my_list[i] = s[i].upper()
        a = "".join(my_list)
    return a

    
if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
